@import url('https://fonts.googleapis.com/css?family=Poppins');

* {
    margin: 0
    padding: 0
}

.header-box 
    background-color: #f47142
    margin: 0 auto

.header-text
    font-family: 'Poppins'
    text-align: center

.header-title
    @extend .header-text
    font-size: 4rem

.header-subtitle
    @extend .header-text
    font-size: 3rem

.body-box
    background-color: #99ffc3
    padding: 30px

.body-text
    font-family: 'Poppins'

.bio-div
    @extend .body-text

.cv-div
    @extend .body-text
    